	"semanticTokenColors": {
		"variable.readonly.defaultLibrary": "{{accent}}",
		"property": "{{foreground}}",
		"parameter": "{{foreground2}}"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "{{comments}}"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "{{comments}}"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "{{comments}}"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "{{errors}}"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "{{operators}}"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "{{keywords}}"
			}
		},
		{
			"name": "Variable Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "{{varTypes}}"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"variable.language"
			],
			"settings": {
				"foreground": "{{langConstants}}"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				// "fontStyle": "underline",
				"foreground": "{{variables}}"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "{{functions}}"
			}
		},
		{
			"name": "Types",
			"scope": [
				"entity.name.type"
			],
			"settings": {
				"foreground": "{{types}}"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				// "fontStyle": "bold",
				"foreground": "{{classes}}"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "{{exceptions}}"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric",
				"constant"
			],
			"settings": {
				"foreground": "{{numeric}}"
			}
		},
		{
			"name": "Characters",
			"scope": [
				"constant.character",
			],
			"settings": {
				"foreground": "{{characters}}"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "{{strings}}"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "{{stringEscape}}" 
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "{{regexp}}"
			}
		},
		{
			"name": "Strings: Symbols",
			"scope": [
				"constant.other.symbol",
				"keyword.operator.expression.arrow"
			],
			"settings": {
				"foreground": "{{symbols}}"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "{{punctuation}}"
			}
		},
		{
			"name": "HTML: Doctype Declaration",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html"
			],
			"settings": {
				"foreground": "{{doctypeDecl}}"
			}
		},
		{
			"name": "HTML: html content",
			"scope": [
				"meta.tag",
				"punctuation.definition.tag.html",
				"text.html.basic", 
				"meta.tag.inline.any.html", 
				"source.html"
			],
			"settings": {
				"foreground": "{{htmlContent}}"
			}
		},
		{
			"name": "HTML: Tags begin and end",
			"scope": [
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.definition.tag.self-close.html"
			],
			"settings": {
				"foreground": "{{tag}}"
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": "entity.name.tag",
			"settings": {
				"fontStyle": "",
				"foreground": "{{tagName}}"
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"foreground": "{{tagAttributeName}}" 
			}
		},
		{
			"name": "HTML: Attribute Values",
			"scope": [
				"meta.tag entity.other.attribute-value",
				"entity.other.attribute-value.html"
			],
			"settings": {
				
				"foreground": "{{tagAttributeValue}}" 
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "{{htmlEntities}}" 
			}
		},
		{
			"name": "CSS: Selectors",
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css",
				"entity.other.attribute-name.id.css"
			],
			"settings": {
				"foreground": "{{cssSelectors}}" 
			}
		},
		{
			"name": "CSS: Property Names",
			"scope": [
				"meta.property-name",
				"support.type.property-name.css",
				"support.type.property-name",
			],
			"settings": {
				"foreground": "{{cssPropertyNames}}" 
			}
		},
		{
			"name": "CSS: Property Values",
			"scope": [
				"meta.property-value.css",
				"meta.property-value",
			],
			"settings": {
				"foreground": "{{cssPropertyValues}}" 
			}
		},
		{
			"name": "CSS: Constants",
			"scope": [
				"constant.css",
				"support.constant",
				"support.constant.property-value.css",
			],
			"settings": {
				"foreground": "{{cssConstants}}" 
			}
		},
		{
			"name": "CSS: Variables",
			"scope": [
				"variable.css"
			],
			"settings": {
				"foreground": "{{cssVariables}}" 
			}
		},
		{
			"name": "CSS: At rules",
			"scope": [
				"keyword.control.at-rule",
				"keyword.control.at-rule.css"
			],
			"settings": {
				// "fontStyle": "bold",
				"foreground": "{{cssAtRules}}" 
			}
		},
		{
			"name": "CSS: Functions",
			"scope": [
				"support.function.misc.css",
			],
			"settings": {
				"foreground": "{{cssFunctions}}" 
			}
		},
		{
			"name": "CSS: Strings",
			"scope": [
				"string.quoted.double.css",
				"string.quoted.single.css"
			],
			"settings": {
				"foreground": "{{cssStrings}}", 
			}
		},
		{
			"name": "CSS: Units",
			"scope": [
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "{{cssUnits}}" 
			}
		},
		{
			"name": "CSS: Symbols",
			"scope": [
				"punctuation.terminator.rule", 
				"punctuation.separator.key-value",
				"punctuation.section.block.css"
			],
			"settings": {
				"foreground": "{{cssSymbols}}" 
			}
		},
		{
			"name": "CSS: Numeric constants",
			"scope": [
				"constant.numeric.css",
				"constant.other.color.rgb-value.hex.css",
			],
			"settings": {
				"foreground": "{{cssNumericConstants}}" 
			}
		},
		{
			"name": "CSS: Important Keyword",
			"scope": "keyword.other.important",
			"settings": {
				"foreground": "{{cssImportantKeyword}}" 
			}
		},
		{
			"name": "Markup: Changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "{{foreground}}"
			}
		},
		{
			"name": "Markup: Deletion",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "{{foreground}}"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: Error",
			"scope": "markup.error",
			"settings": {
				"foreground": "{{errors}}"
			}
		},
		{
			"name": "Markup: Insertion",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "{{foreground}}"
			}
		},
		{
			"name": "Markup: Link",
			"scope": "meta.link",
			"settings": {
				"foreground": "{{links}}"
			}
		},
		{
			"name": "Markup: Output",
			"scope": [
				"markup.output",
				"markup.raw"
			],
			"settings": {
				"foreground": "{{foreground1}}"
			}
		},
		{
			"name": "Markup: Prompt",
			"scope": "markup.prompt",
			"settings": {
				"foreground": "{{foreground1}}"
			}
		},
		{
			"name": "Markup: Heading",
			"scope": "markup.heading",
			"settings": {
				"foreground": "{{headings}}"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Traceback",
			"scope": "markup.traceback",
			"settings": {
				"foreground": "{{errors}}"
			}
		},
		{
			"name": "Markup: Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "{{quotes}}"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "{{lists}}"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "{{stylings}}"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "{{inlineCode}}"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"foreground": "{{foreground1}}"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
