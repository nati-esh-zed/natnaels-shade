{
	"name": "Natnael's shade",
	"colors": {
		"titleBar.activeBackground": "{{titleBarActiveBackground}}",
		"titleBar.inactiveBackground": "{{titleBarInactiveBackground}}",
		"titleBar.border": "{{titleBarBorder}}",

		"editor.background": "{{background}}",
		"editor.foreground": "{{foreground}}", 
		"foreground": "{{foreground}}",

		"statusBar.background": "{{statusBarBackground}}",
		"statusBar.foreground": "{{statusBarForeground}}",
		"statusBar.border": "{{statusBarBorder}}",
		"statusBarItem.remoteBackground": "{{statusBarItemRemoteBackground}}",
		"statusBarItem.activeBackground": "{{statusBarItemActiveBackground}}",
		"statusBarItem.hoverBackground": "{{statusBarItemHoverBackground}}",
		"statusBarItem.hoverForeground": "{{statusBarItemHoverForeground}}",
		"statusBarItem.offlineHoverBackground": "{{statusBarItemHoverBackground}}",
		"statusBarItem.compactHoverBackground": "{{statusBarItemHoverBackground}}",
		"statusBarItem.prominentBackground": "{{statusBarItemProminentBackground}}",

		"sideBar.background": "{{sideBarBackground}}",
		"sideBar.foreground": "{{sideBarForeground}}",
		"sideBarTitle.background": "{{sideBarTitleBackground}}",
		"sideBarTitle.border": "{{sideBarTitleBorder}}",
		"sideBar.border": "{{sideBarBorder}}",
		"sideBarSectionHeader.background": "{{sideBarSectionHeaderBackground}}",
		
		"icon.foreground": "{{iconForeground}}",
		// "list.activeSelectionBackground": "{{listActiveSelectionBackground}}",
		"list.inactiveSelectionBackground": "{{listInactiveSelectionBackground}}",
		"list.activeSelectionForeground": "{{listActiveSelectionForeground}}",
		"list.inactiveSelectionForeground": "{{listInactiveSelectionForeground}}",
		// "list.activeSelectionIconForeground": "{{listActiveSelectionIconForeground}}",
		"list.warningForeground": "{{listWarningForeground}}",
		"list.errorForeground": "{{listErrorForeground}}",

		"activityBar.background": "{{activityBarBackground}}",
		// "activityBar.activeBackground": "{{activityBarActiveBackground}}",
		"activityBar.foreground": "{{activityBarForeground}}",
		"activityBar.inactiveForeground": "{{activityBarInactiveForeground}}",
		"activityBar.border": "{{activityBarBorder}}",

		"editorPane.background": "{{editorPaneBackground}}",
		"editorLineNumber.foreground": "{{editorLineNumberForeground}}",
		"editorLineNumber.activeForeground": "{{editorLineNumberActiveForeground}}",
		"editor.foldBackground": "{{editorFoldBackground}}",

		"panel.background": "{{panelBackground}}",
		"panelTitle.border": "{{panelTitleBorder}}",
		"panelTitle.activeBorder": "{{panelTitleActiveBorder}}",
		"panelTitle.activeForeground": "{{panelTitleActiveForeground}}",
		"panelTitle.inactiveForeground": "{{panelTitleInactiveForeground}}",

		"menu.background": "{{menuBackground}}",
		"menu.foreground": "{{menuForeground}}",

		"quickInput.background": "{{quickInputBackground}}",

		"input.background": "{{inputBackground}}",
		// "banner.background": "{{bannerBackground}}",
		// "gauge.background": "{{gaugeBackground}}",
		"tab.border": "{{tabBorder}}",
		"tab.inactiveBackground": "{{tabInactiveBackground}}",
		"tab.inactiveForeground": "{{tabInactiveForeground}}",
		"tab.activeForeground": "{{foreground}}",
		"editorGroupHeader.tabsBackground": "{{editorGroupHeaderTabsBackground}}",
		"editorGroupHeader.tabsBorder": "{{tabBorder}}",
		
		// "tab.hoverBackground": "{{tabHoverBackground}}",
		// "button.hoverBackground": "{{buttonHoverBackground}}",
		// "toolbar.hoverBackground": "{{toolbarHoverBackground}}",

		"editorBracketHighlight.foreground1": "{{bracket1}}",
		"editorBracketHighlight.foreground2": "{{bracket2}}",
		"editorBracketHighlight.foreground3": "{{bracket3}}",
		"editorBracketHighlight.foreground4": "{{bracket4}}",
		"editorBracketHighlight.unexpectedBracket.foreground": "{{exceptions}}"
	},
	"semanticTokenColors": {
		"variable.readonly.defaultLibrary": "{{accent}}",
		"property": "{{foreground}}",
		"parameter": "{{foreground2}}"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "{{comments}}"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "{{comments}}"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "{{comments}}"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#b60c0c"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "{{operators}}"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "{{keywords}}"
			}
		},
		{
			"name": "Variable Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "{{varTypes}}"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"variable.language"
			],
			"settings": {
				"foreground": "{{langConstants}}"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				// "fontStyle": "underline",
				"foreground": "{{variables}}"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "{{functions}}"
			}
		},
		{
			"name": "Types",
			"scope": [
				"entity.name.type"
			],
			"settings": {
				"foreground": "{{types}}"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				// "fontStyle": "bold",
				"foreground": "{{classes}}"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "{{exceptions}}"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric",
				"constant"
			],
			"settings": {
				"foreground": "{{numeric}}"
			}
		},
		{
			"name": "Characters",
			"scope": [
				"constant.character",
			],
			"settings": {
				"foreground": "{{characters}}"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "{{strings}}"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "{{stringEscape}}" 
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "{{regexp}}"
			}
		},
		{
			"name": "Strings: Symbols",
			"scope": [
				"constant.other.symbol",
				"keyword.operator.expression.arrow"
			],
			"settings": {
				"foreground": "{{symbols}}"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "{{punctuation}}"
			}
		},
		{
			"name": "HTML: Doctype Declaration",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html"
			],
			"settings": {
				"foreground": "{{doctypeDecl}}"
			}
		},
		{
			"name": "HTML: html content",
			"scope": [
				"meta.tag",
				"punctuation.definition.tag.html",
				"text.html.basic", 
				"meta.tag.inline.any.html", 
				"source.html"
			],
			"settings": {
				"foreground": "{{htmlContent}}"
			}
		},
		{
			"name": "HTML: Tags begin and end",
			"scope": [
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.definition.tag.self-close.html"
			],
			"settings": {
				"foreground": "{{tag}}"
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": "entity.name.tag",
			"settings": {
				"fontStyle": "",
				"foreground": "{{tagName}}"
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"foreground": "{{tagAttributeName}}" 
			}
		},
		{
			"name": "HTML: Attribute Values",
			"scope": [
				"meta.tag entity.other.attribute-value",
				"entity.other.attribute-value.html"
			],
			"settings": {
				
				"foreground": "{{tagAttributeValue}}" 
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "{{htmlEntities}}" 
			}
		},
		{
			"name": "CSS: Selectors",
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css",
				"entity.other.attribute-name.id.css"
			],
			"settings": {
				"foreground": "{{cssSelectors}}" 
			}
		},
		{
			"name": "CSS: Property Names",
			"scope": [
				"meta.property-name",
				"support.type.property-name.css",
				"support.type.property-name",
			],
			"settings": {
				"foreground": "{{cssPropertyNames}}" 
			}
		},
		{
			"name": "CSS: Property Values",
			"scope": [
				"meta.property-value.css",
				"meta.property-value",
			],
			"settings": {
				"foreground": "{{cssPropertyValues}}" 
			}
		},
		{
			"name": "CSS: Constants",
			"scope": [
				"constant.css",
				"support.constant",
				"support.constant.property-value.css",
			],
			"settings": {
				"foreground": "{{cssConstants}}" 
			}
		},
		{
			"name": "CSS: Variables",
			"scope": [
				"variable.css"
			],
			"settings": {
				"foreground": "{{cssVariables}}" 
			}
		},
		{
			"name": "CSS: At rules",
			"scope": [
				"keyword.control.at-rule",
				"keyword.control.at-rule.css"
			],
			"settings": {
				// "fontStyle": "bold",
				"foreground": "{{cssAtRules}}" 
			}
		},
		{
			"name": "CSS: Functions",
			"scope": [
				"support.function.misc.css",
			],
			"settings": {
				"foreground": "{{cssFunctions}}" 
			}
		},
		{
			"name": "CSS: Strings",
			"scope": [
				"string.quoted.double.css",
				"string.quoted.single.css"
			],
			"settings": {
				"foreground": "{{cssStrings}}", 
			}
		},
		{
			"name": "CSS: Units",
			"scope": [
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "{{cssUnits}}" 
			}
		},
		{
			"name": "CSS: Symbols",
			"scope": [
				"punctuation.terminator.rule", 
				"punctuation.separator.key-value",
				"punctuation.section.block.css"
			],
			"settings": {
				"foreground": "{{cssSymbols}}" 
			}
		},
		{
			"name": "CSS: Numeric constants",
			"scope": [
				"constant.numeric.css",
				"constant.other.color.rgb-value.hex.css",
			],
			"settings": {
				"foreground": "{{cssNumericConstants}}" 
			}
		},
		{
			"name": "CSS: Important Keyword",
			"scope": "keyword.other.important",
			"settings": {
				"foreground": "{{cssImportantKeyword}}" 
			}
		},
		{
			"name": "Markup: Changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Deletion",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: Error",
			"scope": "markup.error",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Insertion",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Link",
			"scope": "meta.link",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup: Output",
			"scope": [
				"markup.output",
				"markup.raw"
			],
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Prompt",
			"scope": "markup.prompt",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Heading",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#AA3731"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Traceback",
			"scope": "markup.traceback",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#7A3E9D"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "#AB6526"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": "meta.diff.header.to-file",
			"settings": {
				"foreground": "#434343"
			}
		}
	]
}
